        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^24.0.0",
